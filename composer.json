{
    "name": "spawnrad/data-bundle",
    "type": "symfony-bundle",
    "homepage": "http://github.com/spawnrad/SPDataBundle",
    "license": "MIT",
    "description": "Extension Data",
    "keywords": [
        "youtube"
    ],
    "authors": [
        {
            "name": "Spawnrad",
            "email": "spdsdazea@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.4 || ^7.1 || ^7.2",
        "symfony/security-bundle": "^6.4 || ^7.1 || ^7.2",
        "symfony/options-resolver": "^6.4 || ^7.1 || ^7.2",
        "symfony/form": "^6.4 || ^7.1 || ^7.2",
        "symfony/yaml": "^6.4 || ^7.1 || ^7.2",
        "php-http/httplug-bundle": "^1.7 || ^2.1"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.9",
        "symfony/browser-kit": "^6.4 || ^7.1 || ^7.2",
        "symfony/css-selector": "^6.4 || ^7.1 || ^7.2",
        "symfony/property-access": "^6.4 || ^7.1 || ^7.2",
        "symfony/validator": "^6.4 || ^7.1 || ^7.2",
        "symfony/stopwatch": "^6.4 || ^7.1 || ^7.2",
        "symfony/phpunit-bridge": "^6.4 || ^7.1 || ^7.2",
        "symfony/translation": "^6.4 || ^7.1 || ^7.2",
        "friendsofsymfony/user-bundle": "^2.1",
        "php-http/httplug-bundle": "^1.7",
        "php-http/guzzle7-adapter": "^1.0",
        "phpunit/phpunit": "^9.6.11",
        "friendsofphp/php-cs-fixer": "^3.23",
        "symfony/monolog-bundle": "^3.4"
    },
    "conflict": {
        "symfony/security-bundle": ">=6.0,<6.3.4",
        "twig/twig": "<1.34"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "to use Doctrine user provider",
        "php-http/httplug-bundle": "to provide required HTTP client with ease.",
        "symfony/property-access": "to use FOSUB integration with this bundle"
    },
    "autoload": {
        "psr-4": {
            "SP\\Bundle\\DataBundle\\": ""
        }
    },
    "minimum-stability": "dev"
}